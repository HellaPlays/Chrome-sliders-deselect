{
    "manifest_version": 3,
    "name": "Deselect Sliders",
    "version": "1.0",
    "description": "Click to deselect all sliders on the current page.",
    "permissions": ["activeTab", "scripting"],
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    }
  }
  